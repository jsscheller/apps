{
  "name": "pdf-page-picker",
  "version": "0.0.1",
  "interface": "picker",
  "description": "Page picker for PDFs - select, move, rotate, delete and split",
  "source": "https://github.com/jsscheller/apps/blob/master/pdf-page-picker/index.js",
  "issues": "https://github.com/jsscheller/apps/issues",
  "input": {
    "pdfFiles": { "type": ["file"], "label": "PDF Files" },
    "allowSelect": "boolean",
    "allowMove": "boolean",
    "allowRotate": "boolean",
    "allowDelete": "boolean",
    "allowSplit": "boolean"
  },
  "output": {
    "docs": [
      [
        {
          "from": "string",
          "pages": "string"
        }
      ]
    ],
    "rotate": [{ "angle": "number", "pages": "string" }]
  },
  "include": ["node_modules/@jspawn/", "styles.css"]
}
